<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<html lang="en" class="no-js">
<head>
    <% include head %>

</head>
<body>
   <% include menu %>
<div id="wrapper">
   <% include aside %>
   <div id="page-content-wrapper">
	<div class="container-fluid">

		<div class="row title">
			<div class="col-9 text-truncate">
				<h3>Diagnostics</h3>
			</div>
			<div class="col-3 text-truncate">
				<form class="navbar-form" role="search">
					<div class="input-group">
						<input type="text" class="form-control" placeholder="Search" aria-label="Search for..." aria-describedby="basic-addon2">
						<span class="input-group-addon" id="basic-addon2"><i class="material-icons">search</i></span>
					</div>
				</form>
			</div>
		</div>
		<table class="table table-striped">
			<thead>
				<tr>
					<th scope="col">#</th>
					<th scope="col">Code</th>
					<th scope="col">System</th>
					<th scope="col">Manufacture</th>
				</tr>
			</thead>
			<tbody>
			<%UserModel.forEach(function(item) { %>
			<tr>
				<td></td>
			<td>
			<%= item.code %></td>
			<td><%= item.system %></td>
			<td><%= item.manufacturer %></td>
			</tr>
			<% }); %>
			
			</tbody>
				
				
		</table>
		<script>
			function sendNotify(username)
			{
				$.get('/Home/sendNotification?userid='+username, function (data) {
					alert(data);
				});
			}
		</script>

	</div>
    
	 <% include footer %>
	</div>
	<!-- /#page-content-wrapper -->

</div>
	

</body>
</html>
